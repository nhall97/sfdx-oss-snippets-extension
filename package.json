{
    "name": "sfdx-oss-snippets-extension",
    "displayName": "sfdx-oss-snippets-extension",
    "description": "Provides snippets for common sfdx packages when packages / 2nd generation packages may be seperate from your metadata repository",
    "version": "0.0.24",
    "engines": {
        "vscode": "^1.44.0"
    },
    "publisher": "nhall97",
    "repository": {
        "directory": "https://github.com/nhall97/sfdx-oss-snippets-extension"
    },
    "devDependencies": {
        "vsce": "^1.83.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "apex",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    },
    "scripts": {
        "deployToMarketplace": "vsce publish"
    }
}
